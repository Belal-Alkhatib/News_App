<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/news_nav_graph"
    app:startDestination="@id/breakingNewsFragment2">
    <fragment
        android:id="@+id/breakingNewsFragment2"
        android:name="com.b.alkhatib.mvvmnewsapp.ui.BreakingNewsFragment"
        android:label="fragment_breaking_news2"
        tools:layout="@layout/fragment_breaking_news2" >
        <action
            android:id="@+id/action_breakingNewsFragment2_to_articleNewsFragment2"
            app:destination="@id/articleNewsFragment2"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            />
    </fragment>
    <fragment
        android:id="@+id/savedNewsFragment2"
        android:name="com.b.alkhatib.mvvmnewsapp.ui.SavedNewsFragment"
        android:label="SavedNewsFragment" >
        <action
            android:id="@+id/action_savedNewsFragment2_to_articleNewsFragment2"
            app:destination="@id/articleNewsFragment2"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/searchNewsFragment2"
        android:name="com.b.alkhatib.mvvmnewsapp.ui.SearchNewsFragment"
        android:label="SearchNewsFragment" >
        <action
            android:id="@+id/action_searchNewsFragment2_to_articleNewsFragment2"
            app:destination="@id/articleNewsFragment2"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/articleNewsFragment2"
        android:name="com.b.alkhatib.mvvmnewsapp.ui.ArticleNewsFragment"
        android:label="ArticleNewsFragment" />
</navigation>